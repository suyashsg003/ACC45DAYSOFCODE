day-44(suyash goyal)

  #include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n,i,temp;
        cin>>n;
	    string s;
	    cin>>s;
	    for(i=0;i<n-1;i=i+2){
	        temp=s[i];
	        s[i]=s[i+1];
	        s[i+1]=temp;
	    }
	    for(i=0;i<n;i++){
	        int d=s[i]-'a';
            char c = 'z'-d;
            cout<<c;
	    }
        cout<<endl;
	}
	return 0;
}
