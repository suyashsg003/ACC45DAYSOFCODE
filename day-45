day-45(suyash goyal)


  #include <iostream>
#include <limits.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n, x, i, j;
        int max = INT_MIN;
        cin >> n >> x;
        int s[n], r[n];
        for (i = 0; i < n; i++)
        {
            cin >> s[i] >> r[i];
        }
        for (i = 0; i < n; i++)
        {
            if (s[i] <= x)
            {
                if (r[i] > max)
                {
                    max = r[i];
                }
            }
        }
        cout<<max<<endl;
    }
    return 0;
}
