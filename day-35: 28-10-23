  #include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int num,count=0;
	    cin>>num;
	    string s;
	    cin>>s;
	    for(int i=0; i<num-1; i++){
	        if(s[i]>s[i+1]){
	            count++;
	        }
	    }
	    cout<<count<<endl;
	}
	return 0;
}
