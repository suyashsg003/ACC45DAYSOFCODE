day-32(suyash goyal)


  #include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int l,r,m;
	    cin>>l>>r>>m;
	    if(m%l!=0){
              cout<<(((m/l)+1)+m/r)<<endl;
        }
        else{
            cout<<((m/l)+m/r)<<endl;
        }
	}
	return 0;
}
