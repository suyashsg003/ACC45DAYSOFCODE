  #include <iostream>
using namespace std;
void getdata(int arr[3]){
    for(int i=0; i<3; i++){
        cin>>arr[i];
    }
}
int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int count1=0,count2=0;
	    int r1,w1,c1,r2,w2,c2;
	    cin>>r1>>w1>>c1;
	    cin>>r2>>w2>>c2;
	    if(r1>r2){
	        count1++;
	    }
	    else{
	        count2++;
	    }
	     if(w1>w2){
	        count1++;
	    }
	    else{
	        count2++;
	    }
	     if(c1>c2){
	        count1++;
	    }
	    else{
	        count2++;
	    }
	    if(count1>count2){
	        cout<<"A"<<endl;
	    }
	    else{
	        cout<<"B"<<endl;
	    }
	}
	return 0;
}
